extends layout

block layout-content
  table
    thead
      tr 
        th(width = "16.6667%") Name 
        th(width = "16.6667%") DoB 
        th(width = "16.6667%") Start Date
        th(width = "16.6667%") Room 1 End
        th(width = "16.6667%") Room 2 End 
        th(width = "16.6667%") Room 3 End 
    tbody
      each student in studentsArray.length ? studentsArray : ['No Students']
        tr
          td #{student.Name}
          td #{student.DoB}
          td #{student.startDate}
          td #{student.roomOneEnd}
          td #{student.roomTwoEnd}
          td #{student.roomThreeEnd}
