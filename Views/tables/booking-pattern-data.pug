block booking-pattern-data        
    each booking in bookingPatterns ? bookingPatterns : ['No Students']
        tr#dataCells
            td(class = "col1") #{booking.Name}!{"&#32;"} 
            td() 
                - var isChecked=false
                if toggle == "Student"
                    if booking.Mm
                        - var isChecked = true
                if toggle == "Teacher"
                    if booking.Mm == roomNo
                        - var isChecked = true
                label() 
                    div#cell
                        input(type="checkbox" checked= isChecked ? true : false name = booking.Name+"Mm"  ) 
                        div()
                        a() 
